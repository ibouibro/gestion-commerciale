<script>
  
    // JavaScript for filtering the table
    document.getElementById('searchInput').addEventListener('keyup', function() {
          var filter = this.value.toUpperCase();
          var rows = document.querySelectorAll("#clientTable tbody tr");
    
          rows.forEach(function(row) {
            // Get the text content of all the columns in the row
            var rowText = row.textContent.toUpperCase();
    
            // Check if the filter text is found in the row's text
            if (rowText.indexOf(filter) > -1) {
              row.style.display = ""; // Show the row
            } else {
              row.style.display = "none"; // Hide the row
            }
          });
        });
    
    </script>

